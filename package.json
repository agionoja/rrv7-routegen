{
  "name": "rrv7-routegen",
  "version": "0.1.0",
  "description": "Generate and watch routeFile helper for React Router apps",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "rrv7-routegen": "./dist/bin/routegen.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts",
    "dev": "bun run build --watch",
    "test": "bun test"
  },
  "keywords": [
    "react-router",
    "route generator",
    "cli"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.23.8",
    "commander": "^12.0.0",
    "enquirer": "^2.4.1",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "chokidar": "^4.0.3",
    "@types/bun": "latest",
    "tsup": "^8.0.0",
    "execa": "^9.0.0"
  }
}
